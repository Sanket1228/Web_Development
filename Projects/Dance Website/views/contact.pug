extends base.pug

block style
    style 
        include ../static/style1.css

block content
    div.container
        h1 Contact Us
        form(action="/contact",method="post")
            input(type="text",name="name",class="myInput",placeholder="Enter your name")
            input(type="phone",name="phone",class="myInput",placeholder="Enter your phone number ")
            input(type="text",name="age",class="myInput",placeholder="Enter your age") 
            input(type="text",name="address",class="myInput",placeholder="Enter your address") 
            input(type="text",name="desc",class="myInput",placeholder="Enter your description")
            button.btn
                |submit 